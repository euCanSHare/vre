{
    "require": {
        "auth0/auth0-php": "^5.0",
        "cache/mongodb-adapter": "^1.1",
        "justinrainbow/json-schema": "^5.2",
        "league/oauth2-google": "^2.0",
        "mongodb/mongodb": "1.5.1",
        "paragonie/random_compat": "v9.99.99 as v2.0.9",
        "paragonie/sodium_compat": "^1.13",
        "phpseclib/phpseclib": "~3.0",
        "slim/slim": "^3.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "App\\Test\\": "tests"
      }
    },
    "config": {
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
